{{> partials/navbar }}
<div class="container  justify-content-center">
    <div class="row">
        {{#each productos}}
            <div class="col-md-4">
                <div class="card m-3" style="width: 15rem;">
                    <img src="/img/{{this}}.png" alt="{{this}}">
                    <div class="card-body text-center">
                        <h3 class="card-title">{{this}}</h3>
                        <a href="#" type="button" class="btn btn-outline-success producto" id="{{this}}">Agregar al carrito</a>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
</div>
